<body>
    <div id="webchat"></div>
    <script>
        // Replace 'YOUR_PROJECT_ID' with your actual project ID
        const projectID = '67afb110d7ad2f8adf95ee0a';

        // Load the Voiceflow web chat widget
        (function() {
            const script = document.createElement('script');
            script.src = `https://webchat.voiceflow.com/v1/${projectID}/bundle.js`;
            script.async = true;
            document.head.appendChild(script);
        })();
    </script>
</body>
